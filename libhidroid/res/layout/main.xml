<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <LinearLayout android:id="@+id/linearLayout1" android:layout_height="wrap_content" android:layout_width="fill_parent">
        <EditText android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_weight="1" android:id="@+id/test_input" android:text="@string/init_test_input"></EditText>
        <Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_weight="0" android:text="@string/do_test" android:id="@+id/do_test"></Button>
    </LinearLayout>
<TextView  
    android:layout_width="fill_parent" 
    android:layout_height="fill_parent" android:text="@string/notinitialized" android:id="@+id/test_output"/>
</LinearLayout>
